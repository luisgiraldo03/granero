        </activity>

        <activity android:name=".Buscar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                />
            </intent-filter>
        </activity>

        <activity android:name=".Eliminar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                />
            </intent-filter>
        </activity>

        <activity android:name=".Eliminar_Producto">
